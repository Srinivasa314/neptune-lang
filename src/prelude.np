fun import(moduleName) {
    let module = _getModule(moduleName)
    if module {
        return module
    }else {
        moduleName=_resolveModule(_getCallerModule(),moduleName)
        module = _getModule(moduleName)
        if module {
            return module
        } else {
            _compileModule(moduleName)()
            return _getModule(moduleName)
        }
    }
}